<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PremiereHackday" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/green_2E</item>
        <item name="colorPrimaryVariant">@color/green_2E</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/green_1E</item>
        <item name="colorSecondaryVariant">@color/green_1E</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
